@*@model WebApp.Models.SearchUserModelList*@
@addTagHelper*, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4>Users</h4>
<hr />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <table id="tblGrid" style="border:2px solid gray;" class="table">
        <tr>
            <td colspan="4" align="right">
                <input type="text" id="txtSearch" />&nbsp;
                <input type="button" id="btnSearch" value="Search" class="btn btn-primary" />
            </td>
        </tr>
        <tr>
            <td colspan="4">
                <div id="divSearch">
                    @Html.Partial("SearchUser", (List<User>)ViewBag.Users)
                </div>
            </td>
        </tr>
    </table>

    <script>
        $(document).ready(function () {
        });

        $('#btnSearch').click(function () {
            SearchUser();
        });

        $('#txtSearch').keypress(function (event) {
            var keycode = (event.keyCode ? event.keyCode : event.which);
            if (keycode == '13') {
                SearchUser();
            }
        });

        function SearchUser() {
            var element = this;
            var query = $("#txtSearch").val();
            $("#divSearch").load("/PronounceName/SearchUser?searchParam=" + query);

        }

    </script>

   